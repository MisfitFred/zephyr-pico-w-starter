{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Pico Debug (Zephyr)",
      "cwd": "${workspaceFolder}",
      "executable": "${workspaceFolder}/build/zephyr/zephyr.elf",
      "request": "launch",
      "type": "cortex-debug",
      "servertype": "openocd",
      "serverpath": "${userHome}/.pico-sdk/openocd/0.12.0+dev/openocd",
      "searchDir": [
        "${userHome}/.pico-sdk/openocd/0.12.0+dev/scripts"
      ],
      "toolchainPrefix": "arm-zephyr-eabi",
      "armToolchainPath": "${userHome}/zephyr-sdk-0.17.4/arm-zephyr-eabi/bin",
      "device": "RP2040",
      "svdFile": "${userHome}/.pico-sdk/sdk/2.2.0/src/rp2040/hardware_regs/RP2040.svd",
      "configFiles": [
        "interface/cmsis-dap.cfg",
        "target/rp2040.cfg"
      ],
      "runToEntryPoint": "main",
      "openOCDLaunchCommands": [
        "adapter speed 5000"
      ],
      "rtos": "Zephyr",
      "preLaunchTask": "Compile Project"
    }
  ]
}